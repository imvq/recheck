{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "migration:generate": "env-cmd -e prod node --require ts-node/register --require module-alias/register ./node_modules/typeorm/cli.js --config src/database/config.ts migration:generate",
    "migration:run": "env-cmd -e prod node --require ts-node/register --require module-alias/register ./node_modules/typeorm/cli.js --config src/database/config.ts migration:run",
    "prepare-media-dir": "mkdir ./dist/media -p",
    "prepare-templates-dir": "mkdir ./dist/services -p && cp -r src/services/templates dist/services",
    "prepare-dirs": "npm-run-all --parallel prepare-media-dir prepare-templates-dir",
    "prepare-swagger": "swaggerGen -c ./swagger.config.yaml -t",
    "prepare-pm2": "cp ./ecosystem.config.js ./dist",
    "prepare-all": "npm-run-all --parallel prepare-dirs prepare-swagger prepare-pm2",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "dev": "env-cmd -e dev ts-node src/dev-watch",
    "build": "mkdir ./dist/media -p && env-cmd -e prod tsc",
    "start": "node ./dist"
  },
  "_moduleAliases": {
    "@application": "dist/application",
    "@common": "dist/common",
    "@config": "dist/config",
    "@controllers": "dist/controllers",
    "@database": "dist/database",
    "@dto": "dist/dto",
    "@logging": "dist/logging",
    "@services": "dist/services",
    "@typing": "dist/typing",
    "@utils": "dist/utils"
  },
  "keywords": [],
  "author": "Cirill Usachov",
  "license": "UNLICENCED",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/handlebars": "^4.1.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/nodemailer": "^6.4.0",
    "@types/tsc-watch": "^4.2.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "chalk": "^4.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "3.7.5",
    "typescript-rest-swagger": "^1.1.5"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "handlebars": "^4.7.7",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "nodemailer": "^6.5.0",
    "pg": "^8.5.1",
    "rand-token": "^1.0.1",
    "scrapedin": "^1.0.21",
    "source-map-support": "^0.5.19",
    "typeorm": "^0.2.29",
    "typescript-ioc": "^3.2.2",
    "typescript-rest": "^3.0.2",
    "typescript-rest-body-guard": "^2.0.1",
    "typescript-rest-ioc": "^1.0.0",
    "typescript-rest-jwt-guard": "^1.1.3",
    "uuid": "^8.3.2"
  }
}
